{
	"id": "CreateUserRequest",
	"description": "Request for creating a fruster user. Can include any number of custom fields.",
	"additionalProperties": true,
	"properties": {
		"id": {
			"$ref": "User#/properties/id"
		},
		"email": {
			"$ref": "User#/properties/email"
		},
		"password": {
			"type": "string",
			"description": "the password of the user. Is required only if `config.requirePassword` is set to true."
		},
		"firstName": {
			"$ref": "User#/properties/firstName"
		},
		"middleName": {
			"$ref": "User#/properties/middleName"
		},
		"lastName": {
			"$ref": "User#/properties/lastName"
		}
	},
	"required": [
		"email",
		"firstName",
		"lastName"
	]
}
